{
  "name": "vscode-plugin-vuesunflower-snippets",
  "displayName": "Vuesunflower Snippets",
  "description": "Vuesunflower的语法代码片段vscode插件",
  "keywords": [
    "vue-sunflower",
    "snippets"
  ],
  "icon": "images/icon.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "galleryBanner": {
    "color": "#FAE301",
    "theme": "dark"
  },
  "main": "./out/extension",
  "activationEvents": [
    "onLanguage:vuejs"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "html",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "vue",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/mybells/vscode-plugin-vuesunflower-snippets/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mybells/vscode-plugin-vuesunflower-snippets"
  },
  "homepage": "https://github.com/mybells/vscode-plugin-vuesunflower-snippets/blob/master/README.md"
}
